        "drupal/webform": "^6.2@beta",
        "drush/drush": "^10.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "cweagans/composer-patches": false,
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "drupal-composer/drupal-scaffold": true,
            "drupal/console-extend-plugin": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "web/drush/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "patches": {
            "drupal/registration_link": {
                "Multiple entries in menu block": "https://www.drupal.org/files/issues/multiple_entries_in_menu_block-2801297-10_0.patch"
            },
            "drupal/facets": {
                "2910828: Some search results can be out of range": "https://www.drupal.org/files/issues/2018-04-09/some_search_results_can-2910828-19.patch"
            },
            "drupal/core": {
                "Breadcrumb title doesn't reflect view contextual filter's title override": "https://www.drupal.org/files/issues/2018-12-23/drupal-view_title_with_contextual_filters-2787051-28.patch"
            },
            "drupal/request_data_conditions": {
                "Add «Contains» and «Not contains» conditions": "https://www.drupal.org/files/issues/2018-09-11/request_data_conditions-add-contain-ops-2998706-2.patch"
            },
            "drupal/video_embed_field": {
                "Add More Embedding Options": "https://www.drupal.org/files/issues/adds-youtube-and-vimeo-options-for-hiding-elements-2861429-8.patch"
            },
            "drupal/commerce_pado": {
                "Ajax Error with Product Variations and Add Ons": "https://www.drupal.org/files/issues/2018-10-09/commerce_pado-ajax_error_cached_form-3004288.patch"
            },
            "drupal/tvi": {
                "Route Enhancer to fix route match parameters.": "https://www.drupal.org/files/issues/2018-08-17/route_enhancer-2993234-3.patch"
            },
            "drupal/flag": {
                "Flags problem with VBO module in views": "https://www.drupal.org/files/issues/2018-09-18/2893259-27.patch",
                "Enabling a flag adds session cookies to anonymous users": "https://www.drupal.org/files/issues/2019-10-29/2494095-26.patch"
            },
            "drupal/facets_pretty_paths": {
                "Pretty paths do not work in combination with contextual filters.": "https://www.drupal.org/files/issues/2020-09-17/facets_pretty_paths-2952005-6.patch"
            },
            "drupal/commerce": {
                "Allow products to be duplicated": "https://www.drupal.org/files/issues/2022-04-01/allow-products-to-be-duplicated-3042258-20.patch"
            }
        }
        
    }
}
